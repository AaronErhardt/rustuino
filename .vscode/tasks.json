{
  "version": "2.0.0",
  "tasks": [
        {
            "label": "Cargo Build",
            "type": "process",
            "command": "cargo",
            "args": ["build"],
            "problemMatcher": ["$rustc"],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "Cargo Run",
            "type": "process",
            "command": "cargo",
            "args": ["run"],
            "problemMatcher": ["$rustc"],
            "group": "build"
        },
        {
            "label": "Cargo Clean",
            "type": "process",
            "command": "cargo",
            "args": ["clean"],
            "problemMatcher": ["$rustc"],
            "group": "build"
        },
    ]
}
